   "id": "46133def-1cbd-4ce4-b030-720ba7c4b234",
   "metadata": {},
   "outputs": [],
   "source": [
    "def ispointcircle(r, x1, y1, x2, y2):\n",
    "    return math.sqrt((x1-x2)**2 + (y1-y2)**2) < r"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "07c98b65-267c-4a47-b37c-047119c4fbca",
   "metadata": {},
   "outputs": [],
   "source": [
    "radius = 50\n",
    "center = (10, 10)\n",
    "point = (2, 3)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "b4dd2dc7-5a29-4bcf-8305-e612870ee1d8",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "True"
      ]
     },
     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
