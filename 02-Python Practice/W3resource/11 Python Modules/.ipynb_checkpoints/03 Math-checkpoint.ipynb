   "source": [
    "## Trapezoid Area Calculation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "f2730494-e1aa-49e9-b3e1-de9c290a64d7",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter Height of trapezoid: 15\n",
      "Base one value:  6\n",
      "Base two value:  3\n"
     ]
    }
   ],
   "source": [
    "height = float(input(\"Enter Height of trapezoid:\"))\n",
    "base_1 = float(input('Base one value: '))\n",
    "base_2 = float(input('Base two value: '))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "95f60488-c446-4d1d-8cba-78c6a5002d1c",
   "metadata": {},
   "outputs": [],
   "source": [
    "area = ((base_1 + base_2) / 2) * height"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "47163216-b7e8-4f9a-aa56-6ad8e7630a11",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Area of trapezoid is: 67.5\n"
     ]
    }
   ],
   "source": [
    "print(f\"Area of trapezoid is: {area}\")"
   ]
  },
