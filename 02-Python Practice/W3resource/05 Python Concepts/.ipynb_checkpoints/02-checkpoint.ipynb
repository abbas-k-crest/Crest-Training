    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter numerator: 75\n",
      "Enter Denominator 0\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Error: Cannot Divide by zero\n"
     ]
    }
   ],
   "source": [
    "try:\n",
    "    num = int(input(\"Enter numerator:\"))\n",
    "    den = int(input(\"Enter Denominator\"))\n",
    "    result = num / den\n",
    "\n",
    "    print(\"Result:\", result)\n",
    "except ZeroDivisionError:\n",
    "    print(\"Error: Cannot Divide by zero\")"
   ]
